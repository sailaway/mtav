{
    "bgColor": "#aaa",
    "width": 360,
    "height": 480,
    "fps": 24,
    "frameCount": 120,
    "outVideoFile":"image-rotate.mp4",
    "trackers": [{
        "startFrame": 0,
        "endFrame": 120,
        "trackType": "simpleImage",
        "initProperties": {
            "videoAlignPtX": 0.5,
            "videoAlignPtY": 0.5,
            "trackerAlignPtX": 0.5,
            "trackerAlignPtY": 0.5,
            "alpha": 0.8,
            "scale": 1,
            "rotate": 0
        },
        "input": {
            "image": "animation-logo.png"
        },
        "animation": [
            {
                "property": "rotate",
                "startVal": "0",
                "endVal": "1440",
                "startOffsetFrame": 0,
                "endOffsetFrame": 120,
                "interpolator": {
                    "interType": "accelerateDecelerate"
                }
            }
        ]
    }],
}